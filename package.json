{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.15.9",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare && pnpm simple-git-hooks install",
    "lint": "pnpm lint:js && pnpm lint:style",
    "lint:js": "eslint . --fix",
    "lint:style": "stylelint **/*.{vue,css} --fix --ignore-path .gitignore",
    "preview": "nuxt preview",
    "test": "vitest --run",
    "typecheck": "nuxt typecheck"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.12.0",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@iconify-json/fa6-brands": "1.2.5",
    "@iconify-json/fa6-solid": "1.2.3",
    "@iconify-json/fluent-emoji-flat": "1.2.3",
    "@iconify-json/ic": "1.2.2",
    "@iconify-json/mdi": "1.2.3",
    "@iconify-json/pajamas": "1.2.6",
    "@iconify-json/uil": "1.2.3",
    "@nuxt/devtools": "2.4.0",
    "@nuxt/eslint": "1.3.0",
    "@nuxt/fonts": "0.11.2",
    "@nuxt/image": "1.9.0",
    "@nuxt/test-utils": "3.18.0",
    "@nuxt/ui": "3.1.1",
    "@pinia/nuxt": "0.11.0",
    "@vue-macros/nuxt": "1.13.5",
    "@vue/test-utils": "2.4.6",
    "@vueuse/core": "13.1.0",
    "@vueuse/nuxt": "13.1.0",
    "date-fns": "4.1.0",
    "eslint": "9.26.0",
    "eslint-plugin-tailwindcss": "3.18.0",
    "happy-dom": "17.4.6",
    "lint-staged": "15.5.1",
    "nuxt": "3.17.1",
    "nuxt-auth-utils": "0.5.19",
    "nuxt-time": "1.0.3",
    "p-limit": "6.2.0",
    "pinia": "3.0.2",
    "pinia-plugin-persistedstate": "4.2.0",
    "playwright-core": "1.52.0",
    "postcss-html": "1.8.0",
    "simple-git-hooks": "2.13.0",
    "stylelint": "16.19.1",
    "stylelint-config-recommended-vue": "1.6.0",
    "stylelint-config-standard": "37.0.0",
    "v-calendar": "3.1.2",
    "vitest": "3.1.2",
    "vue-tsc": "2.1.6",
    "vue-virtual-scroller": "2.0.0-beta.8",
    "zod": "3.24.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "pnpm commitlint --edit $1"
  },
  "lint-staged": {
    "*.{js,ts,mjs,vue}": [
      "eslint --fix"
    ],
    "*.{css,vue}": "stylelint --fix"
  }
}
